<h1>New regular_operating_time</h1>

<% form_for(@regular_operating_time) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :eatery %><br />
    <%= f.text_field :eatery %>
  </p>
  <p>
    <%= f.label :opensAt %><br />
    <%= f.datetime_select :opensAt %>
  </p>
  <p>
    <%= f.label :closesAt %><br />
    <%= f.datetime_select :closesAt %>
  </p>
  <p>
    <%= f.label :daysOfWeek %><br />
    <%= f.text_field :daysOfWeek %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', regular_operating_times_path %>
